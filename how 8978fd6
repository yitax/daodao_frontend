[32m+[m
[32m+[m[32m@router.post("/batch-confirm", response_model=Dict[str, Any])[m
[32m+[m[32masync def batch_import([m
[32m+[m[32m    request_data: Dict[str, Any] = Body(...),[m
[32m+[m[32m    db: Session = Depends(get_db),[m
[32m+[m[32m    current_user: User = Depends(get_current_user),[m
[32m+[m[32m):[m
[32m+[m[32m    """[m
[32m+[m[32m    æ‰¹é‡ç¡®è®¤å¹¶å¯¼å…¥äº¤æ˜“è®°å½•[m
[32m+[m[32m    """[m
[32m+[m[32m    print("\n\n========= æ¥æ”¶åˆ°æ‰¹é‡ç¡®è®¤è¯·æ±‚ =========")[m
[32m+[m[32m    print(f"è¯·æ±‚æ•°æ®æ¡æ•°: {len(request_data.get('transactions', []))}")[m
[32m+[m
[32m+[m[32m    try:[m
[32m+[m[32m        # éªŒè¯è¯·æ±‚æ•°æ®[m
[32m+[m[32m        if not request_data.get("confirmed", False):[m
[32m+[m[32m            return {[m
[32m+[m[32m                "message": "æ‰¹é‡å¯¼å…¥æœªç¡®è®¤",[m
[32m+[m[32m                "imported_count": 0,[m
[32m+[m[32m                "skipped_count": 0,[m
[32m+[m[32m                "results": [][m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m        transactions = request_data.get("transactions", [])[m
[32m+[m[32m        if not transactions:[m
[32m+[m[32m            return {[m
[32m+[m[32m                "message": "æ²¡æœ‰å¯å¯¼å…¥çš„äº¤æ˜“è®°å½•",[m
[32m+[m[32m                "imported_count": 0,[m
[32m+[m[32m                "skipped_count": 0,[m
[32m+[m[32m                "results": [][m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m        imported_count = 0[m
[32m+[m[32m        skipped_count = 0[m
[32m+[m[32m        results = [][m
[32m+[m[32m        batch_errors = [][m
[32m+[m
[32m+[m[32m        # ä½¿ç”¨äº‹åŠ¡å¤„ç†æ‰¹é‡å¯¼å…¥[m
[32m+[m[32m        try:[m
[32m+[m[32m            for idx, transaction_data in enumerate(transactions, start=1):[m
[32m+[m[32m                try:[m
[32m+[m[32m                    # æ•°æ®é¢„å¤„ç†[m
[32m+[m[32m                    processed_data = {[m
[32m+[m[32m                        "message_id": -1,  # æ‰¹é‡å¯¼å…¥ç‰¹æ®Šæ ‡è¯†[m
[32m+[m[32m                        "confirm": True,[m
[32m+[m[32m                        "type": (transaction_data.get("type") or "").lower(),[m
[32m+[m[32m                        "amount": float(transaction_data.get("amount", 0)),[m
[32m+[m[32m                        "description": transaction_data.get("description", ""),[m
[32m+[m[32m                        "category": transaction_data.get("category", "æœªåˆ†ç±»"),[m
[32m+[m[32m                        "date": transaction_data.get("date"),[m
[32m+[m[32m                        "time": transaction_data.get("time", "00:00")[m
[32m+[m[32m                    }[m
[32m+[m[32m                    # æ—¥æœŸæ ¼å¼å¤„ç†[m
[32m+[m[32m                    if processed_data["date"] and "T" in processed_data["date"]:[m
[32m+[m[32m                        processed_data["date"] = processed_data["date"].split("T")[0][m
[32m+[m[32m                    # éªŒè¯å¿…å¡«å­—æ®µ[m
[32m+[m[32m                    if processed_data["amount"] <= 0:[m
[32m+[m[32m                        raise ValueError("é‡‘é¢æ— æ•ˆ")[m
[32m+[m[32m                    # åˆ›å»ºç¡®è®¤å¯¹è±¡[m
[32m+[m[32m                    confirmation = TransactionConfirmation(**processed_data)[m
[32m+[m[32m                    # è°ƒç”¨å•æ¡ç¡®è®¤æ¥å£[m
[32m+[m[32m                    result = confirm_transaction(confirmation, db, current_user)[m
[32m+[m
[32m+[m[32m                    if result["confirmed"]:[m
[32m+[m[32m                        imported_count += 1[m
[32m+[m[32m                        results.append({[m
[32m+[m[32m                            "status": "success",[m
[32m+[m[32m                            "transaction_id": result["transaction"]["id"],[m
[32m+[m[32m                            "data": transaction_data,[m
[32m+[m[32m                            "message": "å¯¼å…¥æˆåŠŸ"[m
[32m+[m[32m                        })[m
[32m+[m[32m                    else:[m
[32m+[m[32m                        skipped_count += 1[m
[32m+[m[32m                        results.append({[m
[32m+[m[32m                            "status": "skipped",[m
[32m+[m[32m                            "data": transaction_data,[m
[32m+[m[32m                            "message": "äº¤æ˜“æœªç¡®è®¤"[m
[32m+[m[32m                        })[m
[32m+[m[32m                except Exception as e:[m
[32m+[m[32m                    skipped_count += 1[m
[32m+[m[32m                    error_msg = f"ç¬¬{idx}æ¡è®°å½•é”™è¯¯: {str(e)}"[m
[32m+[m[32m                    results.append({[m
[32m+[m[32m                        "status": "failed",[m
[32m+[m[32m                        "data": transaction_data,[m
[32m+[m[32m                        "message": error_msg[m
[32m+[m[32m                    })[m
[32m+[m[32m                    batch_errors.append(error_msg)[m
[32m+[m[32m                    print(f"è·³è¿‡æ— æ•ˆè®°å½•: {error_msg}")[m
[32m+[m
[32m+[m[32m            db.commit()  # æäº¤æ•´ä¸ªæ‰¹é‡äº‹åŠ¡[m
[32m+[m
[32m+[m[32m        except Exception as e:[m
[32m+[m[32m            db.rollback()  # å‡ºé”™æ—¶å›æ»š[m
[32m+[m[32m            raise HTTPException([m
[32m+[m[32m                status_code=400,[m
[32m+[m[32m                detail=f"æ‰¹é‡å¯¼å…¥è¿‡ç¨‹ä¸­å‡ºé”™: {str(e)}"[m
[32m+[m[32m            )[m
[32m+[m[32m        # æ„å»ºå“åº”[m
[32m+[m[32m        response = {[m
[32m+[m[32m            "message": f"æ‰¹é‡å¯¼å…¥å®Œæˆï¼ŒæˆåŠŸå¯¼å…¥{imported_count}æ¡ï¼Œè·³è¿‡{skipped_count}æ¡",[m
[32m+[m[32m            "imported_count": imported_count,[m
[32m+[m[32m            "skipped_count": skipped_count,[m
[32m+[m[32m            "results": results[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if batch_errors:[m
[32m+[m[32m            response["batch_errors"] = batch_errors[:10]  # æœ€å¤šè¿”å›10æ¡é”™è¯¯[m
[32m+[m
[32m+[m[32m        print(f"æ‰¹é‡å¯¼å…¥ç»“æœ: {response['message']}")[m
[32m+[m[32m        return response[m
[32m+[m
[32m+[m[32m    except HTTPException:[m
[32m+[m[32m        raise[m
[32m+[m[32m    except Exception as e:[m
[32m+[m[32m        print(f"æ‰¹é‡å¯¼å…¥è¿‡ç¨‹ä¸­å‡ºé”™: {str(e)}")[m
[32m+[m[32m        import traceback[m
[32m+[m[32m        print(f"é”™è¯¯å †æ ˆ:\n{traceback.format_exc()}")[m
[32m+[m[32m        raise HTTPException([m
[32m+[m[32m            status_code=status.HTTP_500_INTERNAL_SERVER_ERROR,[m
[32m+[m[32m            detail=f"æ‰¹é‡å¯¼å…¥å¤±è´¥: {str(e)}"[m
[32m+[m[32m        )[m
