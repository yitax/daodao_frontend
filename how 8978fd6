[32m+[m[32mimport os[m
[32m+[m[32mimport re[m
[32m+[m[32mimport pandas as pd[m
[32m+[m[32mdef read_file_batch(file_path):[m
[32m+[m[32m    """[m
[32m+[m[32m    è¯»å–æ–‡ä»¶å¹¶è½¬æ¢ä¸ºAIå¤„ç†æ‰€éœ€çš„æ ‡å‡†åŒ–æ–‡æœ¬æ ¼å¼[m
[32m+[m[32m    è¿”å›: çº¯æ–‡æœ¬å­—ç¬¦ä¸² (CSVæ ¼å¼)[m
[32m+[m[32m    """[m
[32m+[m[32m    _, ext = os.path.splitext(file_path)[m
[32m+[m[32m    ext = ext.lower()[m
[32m+[m[32m    if ext in ['.txt', '.csv', '.log']:[m
[32m+[m[32m        # æ–‡æœ¬æ–‡ä»¶ï¼Œå°è¯•å¤šç§ç¼–ç [m
[32m+[m[32m        encodings = ['utf-8-sig', 'utf-8', 'gbk', 'gb18030'][m
[32m+[m[32m        for encoding in encodings:[m
[32m+[m[32m            try:[m
[32m+[m[32m                with open(file_path, 'r', encoding=encoding) as f:[m
[32m+[m[32m                    return f.read()[m
[32m+[m[32m            except UnicodeDecodeError:[m
[32m+[m[32m                continue[m
[32m+[m[32m        raise ValueError(f"æ— æ³•è§£ç æ–‡æœ¬æ–‡ä»¶ {file_path}")[m
[32m+[m
[32m+[m[32m    elif ext in ['.xlsx', '.xls']:[m
[32m+[m[32m        try:[m
[32m+[m[32m            df = pd.read_excel(file_path)  # è¯»å–æ•´ä¸ªExcelæ–‡ä»¶[m
[32m+[m[32m            return df.to_csv(index=False)[m
[32m+[m[32m        except Exception as e:[m
[32m+[m[32m            raise ValueError(f"æ— æ³•è¯»å–Excelæ–‡ä»¶ {file_path}: {str(e)}")[m
[32m+[m[32m    else:[m
[32m+[m[32m        raise ValueError(f"ä¸æ”¯æŒçš„æ–‡ä»¶ç±»å‹: {ext}")[m
